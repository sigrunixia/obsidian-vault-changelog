	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "latest",
		"moment": "^2.30.1",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "latest"
	},
	"dependencies": {
		"process": "^0.11.10"
	}
}
